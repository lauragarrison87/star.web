	keywords = {Information visualization, • Human-centered computing → Graph drawings, literature survey, multilayer networks, network visualization, Visualization systems and tools},
	file = {Full Text PDF:C\:\\Users\\Laura\\Zotero\\storage\\AEJATUVW\\McGee et al. - 2019 - The State of the Art in Multilayer Network Visuali.pdf:application/pdf;Snapshot:C\:\\Users\\Laura\\Zotero\\storage\\9HRC46K7\\cgf.html:text/html}
